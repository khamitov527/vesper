# Vesper Environment Variables
# Make a copy of this file named .env and fill in your own values

# API Keys
OPENAI_API_KEY=your_openai_api_key_here

# Google API Credentials
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URL=https://your-app.com/auth/callback

# Supabase Configuration
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_KEY=your_supabase_service_key_here

# Vercel API Configuration
VERCEL_API_URL=https://your-vercel-app.vercel.app/api

# Google API Key (if needed)
GOOGLE_API_KEY=your_google_api_key_here 